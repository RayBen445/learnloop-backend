{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/health",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
